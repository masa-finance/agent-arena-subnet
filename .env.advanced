# Masa Bittensor Subnet 42 Configuration Settings

# Version tag for masa-bittensor images
TAG=latest

# Bittensor wallet configuration
# Your coldkey mnemonic (KEEP THIS SECURE!)
COLDKEY_MNEMONIC="word1 word2 word3 word4 ... word12"

# Network Configuration
SUBTENSOR_NETWORK=test
SUBTENSOR_ADDRESS=wss://test.finney.opentensor.ai:443
NETUID=249
#NETUID=59 for mainnet

# How many miners to run with your coldkey
MINER_COUNT=3
# Add TWEET_VERIFICATION_ID_N entries for each miner you want to run
# Each ID corresponds to the tweet each of your agents sent containing their public key
TWEET_VERIFICATION_ID_1=your_tweet_id_1
TWEET_VERIFICATION_ID_2=your_tweet_id_2
TWEET_VERIFICATION_ID_3=your_tweet_id_3

# Ports will be incremented for each miner replica
MINER_AXON_PORT=8242
MINER_METRICS_PORT=8101
MINER_GRAFANA_PORT=3101

# Number of validators to run (0-64)
VALIDATOR_COUNT=1
# Ports Configuration
VALIDATOR_AXON_PORT=8142
# Validator metrics port
VALIDATOR_METRICS_PORT=8001
# Validator Grafana port
VALIDATOR_GRAFANA_PORT=3001
VALIDATOR_API_HOST=127.0.0.1
VALIDATOR_API_PORT=8088
ORACLE_BASE_URL=http://127.0.0.1:8088/api/v1
API_KEY=masa_pub_v1_7h4j9k2m5n8p3
API_URL=https://test.protocol-api.masa.ai

# start.sh Cleanup behavior
# Set to true to automatically clean up services on failure
CLEANUP_ON_FAILURE=false